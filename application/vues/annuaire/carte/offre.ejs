
				<div class="card carte">
					<div class="card-image">
						<figure class="image organisation">
							<img src="<%= offre.service.organisation.logotype.adresse %>" alt="<%= false && offre.service.organisation.logotype.existe ? (langue.fiche.logotypeDe + " " + offre.service.organisation.nomComplet) : langue.fiche.sansLogotype %>" />
						</figure>
					</div>
					<div class="card-content">
						<p class="title is-4 is-spaced">
							<a href="/offre/<%= offre._id %>">
								<%= offre.intitulé %>
							</a>
						</p>

						<p class="subtitle is-4 is-unspaced">
							<%= offre.service.organisation.raisonSociale %>
						</p>

						<p class="subtitle is-5">
							<%= langue.travail.typesEmploi[offre.typeEmploi].intituléCapitalisé %> · <%= offre.service.adresse %>
						</p>
					</div>
				</div>